<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core">
	<Dialog
		class="sapUiSizeCompact"
		title="{i18n>EditSemanticObjectFragmentTitle}">
		<content>
			<f:SimpleForm id="idEditForm"
				editable="true"
				layout="ResponsiveGridLayout"
				labelSpanXL="4"
				labelSpanL="4"
				labelSpanM="4"
				labelSpanS="12"
				adjustLabelSpan="false"
				emptySpanXL="1"
				emptySpanL="1"
				emptySpanM="1"
				emptySpanS="0"
				columnsXL="1"
				columnsL="1"
				columnsM="1"
				singleContainerFullSize="false" >
				<f:content>
					<Label text="{i18n>SemanticNameObject}" required="true" />
					<Input name="editSemanticObject" id="semanticObjectEdit" value="{semanticObject}" />
				</f:content>
			</f:SimpleForm>
		</content>
		<buttons>
			<Button type="Emphasized" text="{i18n>EditButton}" press="onEdit" />
			<Button type="Ghost" text="{i18n>CancelButton}" press="onCancel" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>
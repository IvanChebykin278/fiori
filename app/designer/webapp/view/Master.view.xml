<mvc:View
    height="100%"
    displayBlock="true"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
    xmlns:semantic="sap.m.semantic"
	controllerName="fiori.designer.controller.Master"
	id="Master">

	<semantic:MasterPage>

		<IconTabBar id="myTabContainer"
		select="onTabSelected"
		class="sapUiResponsiveContentPadding" headerMode="Inline" applyContentPadding="true" expandable="false" tabDensityMode="Inherit">
			<items>
				<IconTabFilter  text="{i18n>Catalogs}" icon="sap-icon://grid" key="Catalogs">
				</IconTabFilter>
				<IconTabFilter text="{i18n>Groups}" icon="sap-icon://menu2" key="Groups">
				</IconTabFilter>
			</items>
			<content>
			</content>
		</IconTabBar>
		<semantic:customFooterContent>
			<OverflowToolbarButton width="97%" icon="sap-icon://sys-add" text="" type="Emphasized" press="onPressAdd"/>
		</semantic:customFooterContent>
		
		
	</semantic:MasterPage>
	
</mvc:View>
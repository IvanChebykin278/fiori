{
  "_version": "1.40.0",
  "sap.app": {
    "id": "fiori.designer",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap-ux/fiori-freestyle-writer:basic",
      "version": "0.11.15",
      "toolsId": "088adf0d-66b9-4830-828c-2772d10a4b67"
    },
    "dataSources": {
      "mainService": {
        "uri": "/v2/service/PageBuilder",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "pageBuilderService": {
        "uri": "/v2/service/PageBuilder",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      },
      "selectedItem": {
        "uri": "/model/selectedItem.json",
        "type": "JSON"
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "selectedItem": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "selectedItem"
      },

      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "fiori.designer.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
			"config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "fiori.designer.view",
        "controlId": "app",
        "clearControlAggregation": true
			},
			"routes": [
				{
					"name": "main",
					"pattern": "",
					"target": [
            "catalogsList",
						"catalogsDetails"
					]
				},
				{
					"name": "CatalogsDetails",
					"pattern": "catalogs/:ID:",
					"target": [
            "catalogsList",
						"catalogsDetails"
					]
				},
        {
					"name": "GroupsDetails",
					"pattern": "groups/:ID:",
					"target": [
            "groupsList",
						"groupsDetails"
					]
				}
			],
			"targets": {
				"Master": {
					"viewName": "Master",
          "viewId": "Master",
          "controlAggregation": "masterPages"
				},
				"catalogsDetails": {
					"viewName": "CatalogsDetails",
          "viewId": "catalogsDetails",
          "controlAggregation": "detailPages"
				},
        "groupsDetails": {
					"viewName": "GroupsDetails",
          "viewId": "groupsDetails",
          "controlAggregation": "detailPages"
				},
        "groupsList": {
					"viewName": "GroupsMasterList",
          "parent": "Master",
					"controlId": "myTabContainer",
          "viewId": "groupsMasterList",
          "controlAggregation": "content",
          "clearAggregation": true
				},
        "catalogsList": {
					"viewName": "CatalogsMasterList",
          "parent": "Master",
					"controlId": "myTabContainer",
          "viewId": "catalogsMasterList",
          "controlAggregation": "content",
          "clearAggregation": true
				}
			}
		},
    "rootView": {
      "viewName": "fiori.designer.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}

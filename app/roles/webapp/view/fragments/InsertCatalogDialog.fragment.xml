<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core">
	<Dialog
		class="sapUiSizeCompact"
		title="Insert role">
		<customData>
			<core:CustomData key="dialogName" value="InsertCatalogDialog" />
		</customData>
		<content>
			<f:SimpleForm
				editable="true"
				layout="ResponsiveGridLayout"
				labelSpanXL="4"
				labelSpanL="4"
				labelSpanM="4"
				labelSpanS="12"
				adjustLabelSpan="false"
				emptySpanXL="1"
				emptySpanL="1"
				emptySpanM="1"
				emptySpanS="0"
				columnsXL="1"
				columnsL="1"
				columnsM="1"
				singleContainerFullSize="false" >
				<f:content>
					<Label text="Catalog ID" required="true" />
					<Input
                        showSuggestion="true"
                        suggest="onSuggest"
                        suggestionItems="{pageBuilder>/Catalogs}"
                        value="{catalogId}">
                        <suggestionItems>
                            <core:Item text="{pageBuilder>ID}" />
                        </suggestionItems>
                    </Input>
				</f:content>
			</f:SimpleForm>
		</content>
		<buttons>
			<Button text="Ok" press="onInsert" />
			<Button text="Cancel" press="onCancel" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>